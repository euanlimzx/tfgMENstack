<% layout('boilerplate')%>
<!-- JUMBOTRON -->
<div
  class="px-4 pt-5 mt-5 text-center d-flex flex-column justify-content-center jumbotron-background text-white"
  style="height: 90vh"
>
  <div>
    <span class="font-4 jumbotron-header" style="font-weight: 600"
      >Tobacco Free
    </span>
    <span
      id="element"
      class="font-4 jumbotron-header"
      style="font-weight: 600"
    ></span>
    <div class="col-lg-6 mx-auto font-2">
      <p class="p-3 jumbotron-subheader">
        A social movement that re-balances the landscape of teenage smoking and
        vaping addiction.
      </p>
      <button class="contact-us mx-auto">
        <div class="svg-wrapper-1">
          <div class="svg-wrapper">
            <svg
              height="20"
              width="20"
              viewBox="0 0 24 24"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path d="M0 0h24v24H0z" fill="none"></path>
              <path
                d="M1.946 9.315c-.522-.174-.527-.455.01-.634l19.087-6.362c.529-.176.832.12.684.638l-5.454 19.086c-.15.529-.455.547-.679.045L12 14l6-8-8 6-8.054-2.685z"
                fill="currentColor"
              ></path>
            </svg>
          </div>
        </div>
        <span>Join Us</span>
      </button>
    </div>
  </div>
</div>
<!-- IMPACT -->
